<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#FF000000</color>
    <color name="colorAccent">#03DAC5</color>
    <color name="colorPrimary">#6200EE</color>
    <color name="colorPrimaryDark">#3700B3</color>
    <color name="lrc_current_text_color">#FF4081</color>
    <color name="lrc_normal_text_color">#9E9E9E</color>
    <color name="lrc_time_text_color">#809E9E9E</color>
    <color name="lrc_timeline_color">#809E9E9E</color>
    <color name="lrc_timeline_text_color">#F8BBD0</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="lrc_divider_height">16dp</dimen>
    <dimen name="lrc_drawable_width">30dp</dimen>
    <dimen name="lrc_text_size">16sp</dimen>
    <dimen name="lrc_time_text_size">12sp</dimen>
    <dimen name="lrc_time_width">40dp</dimen>
    <dimen name="lrc_timeline_height">1dp</dimen>
    <integer name="lrc_animation_duration">1000</integer>
    <string name="app_dir">_iMusic</string>
    <string name="app_name">千千静听</string>
    <string name="lrc">
        喝纯白的豆浆 是纯白的浪漫\n
        望着你可爱脸庞 和你纯真的模样\n
        我傻傻对你笑 是你有种解药\n
        你说我就像油条 很简单却很美好\n
        我知道你和我就像是豆浆油条\n
        要一起吃下去味道才会是最好\n
        你需要我的傻笑 我需要你的拥抱\n
        爱情就需要这样 它才不会单调关于\n
    </string>
    <string name="lrc_label">暂无歌词</string>
    <string name="title_dashboard">歌手</string>
    <string name="title_home">音乐</string>
    <string name="title_mv">MV</string>
    <string name="title_notifications">关于</string>
    <style name="Theme.IMusic" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <declare-styleable name="LrcView">
        <attr format="dimension" name="lrcTextSize"/>
        <attr format="dimension" name="lrcNormalTextSize"/>
        <attr format="dimension" name="lrcDividerHeight"/>
        <attr format="reference|color" name="lrcNormalTextColor"/>
        <attr format="reference|color" name="lrcCurrentTextColor"/>
        <attr format="reference|color" name="lrcTimelineTextColor"/>
        <attr format="integer" name="lrcAnimationDuration"/>
        <attr format="string" name="lrcLabel"/>
        <attr format="dimension" name="lrcPadding"/>
        <attr format="reference|color" name="lrcTimelineColor"/>
        <attr format="dimension" name="lrcTimelineHeight"/>
        <attr format="reference" name="lrcPlayDrawable"/>
        <attr format="reference|color" name="lrcTimeTextColor"/>
        <attr format="dimension" name="lrcTimeTextSize"/>
        <attr name="lrcTextGravity">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="crilProgress">
        <attr format="color" name="initialColor"/>
        <attr format="color" name="finishColor"/>
    </declare-styleable>
    <declare-styleable name="musicView">
        <attr format="color" name="itemColor"/>
    </declare-styleable>
    <declare-styleable name="waterProgress">
        <attr format="color" name="bgColor"/>
        <attr format="color" name="textColor"/>
    </declare-styleable>
</resources>